; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Flock"
run/main_scene="res://_scenes/main.tscn"
run/enable_alt_space_menu=true
config/features=PackedStringArray("4.3", "GL Compatibility")
config/icon="res://icon.svg"

[autoload]

SignalBus="*res://_scripts/signal_bus.gd"
WaveResourceGlobal="*res://_scripts/wave_resource.gd"
GlobalPaths="*res://_scripts/global_paths.gd"
SceneManager="*res://_scripts/SceneManager.gd"

[debug]

settings/stdout/print_fps=true
settings/stdout/print_gpu_profile=true
settings/stdout/verbose_stdout=true
gdscript/warnings/unused_variable=0
gdscript/warnings/unused_parameter=0
gdscript/warnings/unused_signal=0
shapes/navigation/geometry_face_color=Color(0.5, 1, 1, 0.760784)
shapes/navigation/geometry_face_disabled_color=Color(0.87, 0.87, 0.87, 1)
shapes/navigation/agent_path_point_size=32.0

[display]

window/size/viewport_width=900
window/size/viewport_height=900
window/size/always_on_top=true
window/size/transparent=true
window/stretch/aspect="keep_width"
display_server/driver.windows="windows"
display_server/driver.android="android"
display_server/driver.ios="iOS"
display_server/driver.macos="macos"

[editor]

version_control/plugin_name="GitPlugin"
version_control/autoload_on_startup=true

[editor_plugins]

enabled=PackedStringArray("res://addons/3dEnemyToolkit/plugin.cfg", "res://addons/3d_controls_toolkit/plugin.cfg")

[file_customization]

folder_colors={
"res://_materials/": "green",
"res://_resources/": "pink",
"res://_scenes/": "orange",
"res://_scripts/": "purple",
"res://_shaders/": "blue"
}

[filesystem]

import/blender/enabled=false

[global]

_global_script_classes=[{
"base": "Resource",
"class": &"BaseBehavior",
"language": "GDScript",
"path": "res://_scripts/behaviors/BaseBehavior.gd"
}, {
"base": "BaseBehavior",
"class": &"BehaviorOnEnterLerp",
"language": "GDScript",
"path": "res://_scripts/behaviors/BehaviorOnEnterLerp.gd"
}]
_global_script_class_icons={}

[global_group]

shield=""
player=""
enemies=""

[input]

move_forward={
"deadzone": 0.5,
"events": [Object(InputEventKey,"resource_local_to_scene":false,"resource_name":"","device":0,"window_id":0,"alt_pressed":false,"shift_pressed":false,"ctrl_pressed":false,"meta_pressed":false,"pressed":false,"keycode":0,"physical_keycode":87,"key_label":0,"unicode":0,"location":0,"echo":false,"script":null)
]
}
